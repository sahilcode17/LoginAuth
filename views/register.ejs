 
<h1>register</h1>
<%- include('./partials/messages.ejs') %>
      
     
                
            
<form action="/users/register" method="post">
    <div>
     <label for="name">name</label>
    <input type="name" name="name" id="name" placeholder="enter name" value="<%= typeof name !='undefined' ? name: ''   %> ">
    </div>
    <div>
    <label for="email">Email</label>
    <input type="email" name="email" id="email" placeholder="enter email id " value="<%= typeof name !='undefined' ? name: ''   %> ">
    </div>
    <div>
    <label for="password">Password</label>
    <input type="password" name="password" id="password" placeholder="enter Password" value="<%= typeof name !='undefined' ? name: ''   %> ">
    </div>
    <div>
    <label for="repassword">Confirm Password</label>
    <input type="password" name="repassword" id="repassword" placeholder="Re-enter Password" value="<%= typeof name !='undefined' ? name: ''   %> ">
    </div>
    <div><button>submit</button></div>
    <p>Already a user ! <a href="/users/login">login </a></p>
   




</form>